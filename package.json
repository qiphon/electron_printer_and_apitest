{
  "name": "electron-quasar-demo",
  "version": "1.0.0",
  "description": "Electron and Quasar demo with cross-platform support for Windows and Android",
  "main": "src-electron/electron-main.js",
  "scripts": {
    "lint": "eslint --ext .js,.vue ./",
    "format": "prettier --write \"**/*.{js,vue,scss,html,md,json}\" --ignore-path .gitignore",
    "dev": "quasar dev -m electron",
    "dev:dbg": "quasar dev -m electron --debug",
    "dev:android": "quasar dev -m capacitor -T android",
    "build": "quasar build -m electron",
    "build:win": "quasar build -m electron --target win",
    "build:mac": "quasar build -m electron --target mac",
    "build:all": "quasar build -m electron --target all",
    "build:android": "quasar build -m capacitor -T android",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@quasar/extras": "^1.16.9",
    "axios": "^1.6.2",
    "bonjour-service": "^1.3.0",
    "electron-updater": "^6.1.7",
    "idb": "^7.1.1",
    "quasar": "^2.14.0",
    "vue": "^3.3.8",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@capacitor/android": "^5.5.1",
    "@capacitor/cli": "^5.5.1",
    "@capacitor/core": "^5.5.1",
    "@qiphon/prettier-config": "^1.0.1",
    "@quasar/app-vite": "^1.7.1",
    "@quasar/cli": "^2.3.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "electron": "^27.1.3",
    "electron-builder": "^24.9.1",
    "electron-packager": "^17.1.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-vue": "^9.19.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2"
  },
  "author": "Qiphon",
  "license": "MIT",
  "private": false,
  "prettier": "@qiphon/prettier-config"
}